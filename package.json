{
  "name": "aik-front",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:full": "concurrently -n \"SERVER,VITE\" -c \"cyan,green\" \"npm run local-server\" \"npm run dev:with-local\"",
    "dev:with-local": "cross-env VITE_ENABLE_LOCAL_FEATURES=true vite",
    "local-server": "cd no-internet-scripts/server && npm start",
    "build": "vite build",
    "preview": "vite preview",
    "check:env": "node scripts/check-env.js",
    "lint": "eslint . --ext .js,.jsx",
    "format": "prettier --write .",
    "test": "node --loader ./test/css-loader.js --loader @esbuild-kit/esm-loader --test",
    "deploy": "bash scripts/deploy.sh"
  },
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-player": "^3.4.0",
    "react-router-dom": "^6.30.1"
  },
  "devDependencies": {
    "@esbuild-kit/esm-loader": "^2.6.5",
    "@testing-library/react": "^14.2.1",
    "@vitejs/plugin-react": "^4.2.0",
    "concurrently": "^8.2.2",
    "cross-env": "^10.1.0",
    "eslint": "^8.57.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^24.0.0",
    "prettier": "^3.3.0",
    "vite": "^5.0.0"
  }
}
